{
  "name": "short-links",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:functions": "npm run build:frontend && node scripts/init-local-db.js && wrangler pages dev dist --port=8788",
    "build": "tsc && vite build && node scripts/export-static.js",
    "build:frontend": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && npx wrangler pages deploy dist"
  },
  "dependencies": {
    "nanoid": "^5.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "wrangler": "^4.62.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250110.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.0.7"
  }
}
